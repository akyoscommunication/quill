  "description": "File input rich editor text for Symfony Forms",
  "license": "MIT",
  "version": "1.1.0",
  "main": "dist/controller.js",
  "types": "dist/controller.d.ts",
  "config": {
    "css_source": "src/style.css"
  },
  "symfony": {
    "controllers": {
